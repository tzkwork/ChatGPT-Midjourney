{
    "github": {
        "silent": true
    },
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Real-IP",
                    "value": "$remote_addr"
                },
                {
                    "key": "X-Forwarded-For",
                    "value": "$proxy_add_x_forwarded_for"
                },
                {
                    "key": "Host",
                    "value": "$http_host"
                }
            ]
        }
    ],
    "routes": [
        {
            "src": "/api/openai/(.*)",
            "headers": { "Host": "api.chatanywhere.com.cn" },
            "dest": "https://api.chatanywhere.com.cn/$1"
        }
    ]
}
